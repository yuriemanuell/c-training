#include <stdio.h>

int main() {
    
    float num1 = 5, num2 = 9, num3 = 7; // Declaração e atribuição de valores às variáveis
    float media;    // Variável para armazenar a média

    media = (num1 + num2 + num3) / 3; // Calculando a média aritmética

    printf("A média aritmética é %.2f\n", media); // Exibindo o resultado com 2 casas decimais

    return 0;
}
